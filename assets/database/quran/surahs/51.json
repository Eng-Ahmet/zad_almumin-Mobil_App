 وَجْهَهَا وَقَالَتْ عَجُوزٌ عَقِيمٌۭ","juz":26,"page":521,"sajda":false},{"numberInQuran":4705,"numberInSurah":30,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4705.mp3","text":"قَالُوا۟ كَذَٰلِكِ قَالَ رَبُّكِ ۖ إِنَّهُۥ هُوَ ٱلْحَكِيمُ ٱلْعَلِيمُ","juz":26,"page":521,"sajda":false},{"numberInQuran":4706,"numberInSurah":31,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4706.mp3","text":"۞ قَالَ فَمَا خَطْبُكُمْ أَيُّهَا ٱلْمُرْسَلُونَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4707,"numberInSurah":32,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4707.mp3","text":"قَالُوٓا۟ إِنَّآ أُرْسِلْنَآ إِلَىٰ قَوْمٍۢ مُّجْرِمِينَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4708,"numberInSurah":33,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4708.mp3","text":"لِنُرْسِلَ عَلَيْهِمْ حِجَارَةًۭ مِّن طِينٍۢ","juz":27,"page":522,"sajda":false},{"numberInQuran":4709,"numberInSurah":34,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4709.mp3","text":"مُّسَوَّمَةً عِندَ رَبِّكَ لِلْمُسْرِفِينَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4710,"numberInSurah":35,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4710.mp3","text":"فَأَخْرَجْنَا مَن كَانَ فِيهَا مِنَ ٱلْمُؤْمِنِينَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4711,"numberInSurah":36,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4711.mp3","text":"فَمَا وَجَدْنَا فِيهَا غَيْرَ بَيْتٍۢ مِّنَ ٱلْمُسْلِمِينَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4712,"numberInSurah":37,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4712.mp3","text":"وَتَرَكْنَا فِيهَآ ءَايَةًۭ لِّلَّذِينَ يَخَافُونَ ٱلْعَذَابَ ٱلْأَلِيمَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4713,"numberInSurah":38,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4713.mp3","text":"وَفِى مُوسَىٰٓ إِذْ أَرْسَلْنَٰهُ إِلَىٰ فِرْعَوْنَ بِسُلْطَٰنٍۢ مُّبِينٍۢ","juz":27,"page":522,"sajda":false},{"numberInQuran":4714,"numberInSurah":39,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4714.mp3","text":"فَتَوَلَّىٰ بِرُكْنِهِۦ وَقَالَ سَٰحِرٌ أَوْ مَجْنُونٌۭ","juz":27,"page":522,"sajda":false},{"numberInQuran":4715,"numberInSurah":40,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4715.mp3","text":"فَأَخَذْنَٰهُ وَجُنُودَهُۥ فَنَبَذْنَٰهُمْ فِى ٱلْيَمِّ وَهُوَ مُلِيمٌۭ","juz":27,"page":522,"sajda":false},{"numberInQuran":4716,"numberInSurah":41,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4716.mp3","text":"وَفِى عَادٍ إِذْ أَرْسَلْنَا عَلَيْهِمُ ٱلرِّيحَ ٱلْعَقِيمَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4717,"numberInSurah":42,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4717.mp3","text":"مَا تَذَرُ مِن شَىْءٍ أَتَتْ عَلَيْهِ إِلَّا جَعَلَتْهُ كَٱلرَّمِيمِ","juz":27,"page":522,"sajda":false},{"numberInQuran":4718,"numberInSurah":43,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4718.mp3","text":"وَفِى ثَمُودَ إِذْ قِيلَ لَهُمْ تَمَتَّعُوا۟ حَتَّىٰ حِينٍۢ","juz":27,"page":522,"sajda":false},{"numberInQuran":4719,"numberInSurah":44,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4719.mp3","text":"فَعَتَوْا۟ عَنْ أَمْرِ رَبِّهِمْ فَأَخَذَتْهُمُ ٱلصَّٰعِقَةُ وَهُمْ يَنظُرُونَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4720,"numberInSurah":45,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4720.mp3","text":"فَمَا ٱسْتَطَٰعُوا۟ مِن قِيَامٍۢ وَمَا كَانُوا۟ مُنتَصِرِينَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4721,"numberInSurah":46,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4721.mp3","text":"وَقَوْمَ نُوحٍۢ مِّن قَبْلُ ۖ إِنَّهُمْ كَانُوا۟ قَوْمًۭا فَٰسِقِينَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4722,"numberInSurah":47,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4722.mp3","text":"وَٱلسَّمَآءَ بَنَيْنَٰهَا بِأَيْي۟دٍۢ وَإِنَّا لَمُوسِعُونَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4723,"numberInSurah":48,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4723.mp3","text":"وَٱلْأَرْضَ فَرَشْنَٰهَا فَنِعْمَ ٱلْمَٰهِدُونَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4724,"numberInSurah":49,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4724.mp3","text":"وَمِن كُلِّ شَىْءٍ خَلَقْنَا زَوْجَيْنِ لَعَلَّكُمْ تَذَكَّرُونَ","juz":27,"page":522,"sajda":false},{"numberInQuran":4725,"numberInSurah":50,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4725.mp3","text":"فَفِرُّوٓا۟ إِلَى ٱللَّهِ ۖ إِنِّى لَكُم مِّنْهُ نَذِيرٌۭ مُّبِينٌۭ","juz":27,"page":522,"sajda":false},{"numberInQuran":4726,"numberInSurah":51,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4726.mp3","text":"وَلَا تَجْعَلُوا۟ مَعَ ٱللَّهِ إِلَٰهًا ءَاخَرَ ۖ إِنِّى لَكُم مِّنْهُ نَذِيرٌۭ مُّبِينٌۭ","juz":27,"page":522,"sajda":false},{"numberInQuran":4727,"numberInSurah":52,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4727.mp3","text":"كَذَٰلِكَ مَآ أَتَى ٱلَّذِينَ مِن قَبْلِهِم مِّن رَّسُولٍ إِلَّا قَالُوا۟ سَاحِرٌ أَوْ مَجْنُونٌ","juz":27,"page":523,"sajda":false},{"numberInQuran":4728,"numberInSurah":53,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4728.mp3","text":"أَتَوَاصَوْا۟ بِهِۦ ۚ بَلْ هُمْ قَوْمٌۭ طَاغُونَ","juz":27,"page":523,"sajda":false},{"numberInQuran":4729,"numberInSurah":54,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4729.mp3","text":"فَتَوَلَّ عَنْهُمْ فَمَآ أَنتَ بِمَلُومٍۢ","juz":27,"page":523,"sajda":false},{"numberInQuran":4730,"numberInSurah":55,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4730.mp3","text":"وَذَكِّرْ فَإِنَّ ٱلذِّكْرَىٰ تَنفَعُ ٱلْمُؤْمِنِينَ","juz":27,"page":523,"sajda":false},{"numberInQuran":4731,"numberInSurah":56,"audio":"https://cdn.islamic.network/quran/audio/128/ar.alafasy/4731.mp3","text":"وَمَا خَلَقْتُ ٱلْجِنَّ وَٱلْإِنسَ إِلَّا 